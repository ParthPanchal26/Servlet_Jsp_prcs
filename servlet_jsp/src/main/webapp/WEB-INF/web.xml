	</servlet>
	<servlet-mapping>
		<servlet-name>math</servlet-name>
		<url-pattern>/math</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>square</servlet-name>
		<servlet-class>com.parth.square</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>square</servlet-name>
		<url-pattern>/square</url-pattern>
	</servlet-mapping>

</web-app>